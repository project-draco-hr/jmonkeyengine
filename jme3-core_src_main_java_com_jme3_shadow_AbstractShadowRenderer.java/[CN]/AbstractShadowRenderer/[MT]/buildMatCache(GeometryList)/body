{
  matCache.clear();
  for (int i=0; i < l.size(); i++) {
    Material mat=l.get(i).getMaterial();
    if (mat.getMaterialDef().getTechniqueDef(postTechniqueName) != null) {
      if (!matCache.contains(mat)) {
        matCache.add(mat);
      }
    }
 else {
      needsfallBackMaterial=true;
    }
  }
}
