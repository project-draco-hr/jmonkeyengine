  ragdoll.setJointLimit("Waist",eighth_pi,eighth_pi,eighth_pi,eighth_pi,eighth_pi,eighth_pi);
  ragdoll.setJointLimit("Chest",eighth_pi,eighth_pi,0,0,eighth_pi,eighth_pi);
}
