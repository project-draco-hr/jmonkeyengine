{
  StringBuilder sb=new StringBuilder();
  sb.append("Uniform[name=");
  sb.append(name);
  if (varType != null) {
    sb.append(", type=");
    sb.append(varType);
    sb.append(", value=");
    sb.append(value);
  }
 else {
    sb.append(", value=<not set>");
  }
  sb.append("]");
  return sb.toString();
}
