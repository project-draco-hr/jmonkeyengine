{
  if (btnDecompress.getText().equals("Cancel")) {
    if (workThread != null) {
      workThread.interrupt();
      System.out.println("User canceled decompression");
      endWork();
    }
  }
 else {
    runNVDecompressAll();
    btnDecompress.setEnabled(true);
    btnDecompress.setText("Cancel");
  }
}
