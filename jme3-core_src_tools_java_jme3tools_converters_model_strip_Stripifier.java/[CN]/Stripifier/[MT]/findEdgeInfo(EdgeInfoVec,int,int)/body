{
  EdgeInfo infoIter=edgeInfos.at(v0);
  while (infoIter != null) {
    if (infoIter.m_v0 == v0) {
      if (infoIter.m_v1 == v1)       return infoIter;
      infoIter=infoIter.m_nextV0;
    }
 else {
      if (infoIter.m_v0 == v1)       return infoIter;
      infoIter=infoIter.m_nextV1;
    }
  }
  return null;
}
