{
  String protocol=null;
  String host=null;
  int port=-1;
  String file=null;
  String authority=null;
  String ref=null;
  for (Iterator it=l.iterator(); it.hasNext(); ) {
    Object elem=(Object)it.next();
    if (elem instanceof SerParser.NameValue) {
      SerParser.NameValue nv=(SerParser.NameValue)elem;
      if (nv.value != null && nv.name != null) {
        if (nv.name.name.equals("port")) {
          port=((Integer)nv.value).intValue();
        }
 else         if (nv.name.name.equals("file")) {
          file=nv.value.toString();
        }
 else         if (nv.name.name.equals("authority")) {
          authority=nv.value.toString();
        }
 else         if (nv.name.name.equals("host")) {
          host=nv.value.toString();
        }
 else         if (nv.name.name.equals("protocol")) {
          protocol=nv.value.toString();
        }
 else         if (nv.name.name.equals("ref")) {
          ref=nv.value.toString();
        }
      }
    }
  }
  return new URL(protocol,host,port,file);
}
