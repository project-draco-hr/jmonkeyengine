{
  if (rewind) {
    setTime(0);
    if (control.getSkeleton() != null) {
      control.getSkeleton().resetAndUpdate();
    }
 else {
      TempVars vars=TempVars.get();
      update(0,vars);
      vars.release();
    }
  }
  animation=null;
  System.out.println("Setting notified false");
  notified=false;
}
