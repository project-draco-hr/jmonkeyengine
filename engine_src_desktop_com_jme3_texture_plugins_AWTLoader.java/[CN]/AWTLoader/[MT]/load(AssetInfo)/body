{
  if (ImageIO.getImageWritersBySuffix(info.getKey().getExtension()) != null) {
    boolean flip=((TextureKey)info.getKey()).isFlipY();
    InputStream in=null;
    try {
      in=info.openStream();
      Image img=load(in,flip);
      return img;
    }
  finally {
      if (in != null) {
        in.close();
      }
    }
  }
  return null;
}
