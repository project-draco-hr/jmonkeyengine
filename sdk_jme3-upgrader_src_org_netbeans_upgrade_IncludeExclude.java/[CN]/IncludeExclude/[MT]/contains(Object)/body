{
  String s=(String)o;
  boolean yes=false;
  Iterator<Union2<Boolean,Pattern>> it=patterns.iterator();
  while (it.hasNext()) {
    Boolean include=it.next().first();
    Pattern p=it.next().second();
    Matcher m=p.matcher(s);
    if (m.matches()) {
      yes=include.booleanValue();
      if (!yes) {
        return false;
      }
    }
  }
  return yes;
}
