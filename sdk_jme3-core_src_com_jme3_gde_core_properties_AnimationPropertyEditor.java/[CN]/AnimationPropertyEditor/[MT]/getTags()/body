{
  if (control == null)   return new String[]{"none"};
  Collection<String> names=control.getAnimationNames();
  String[] strings=new String[names.size() + 1];
  strings[0]="null";
  int i=1;
  for (Iterator<String> it=names.iterator(); it.hasNext(); ) {
    strings[i]=it.next();
    i++;
  }
  return strings;
}
