{
  if (linkChildren.readOnly) {
    return new Action[]{SystemAction.get(CopyAction.class)};
  }
 else {
    return new Action[]{new NewControlPopup(this),Actions.alwaysEnabled(new AddUserDataAction(this),"Add User Data","",false),SystemAction.get(RenameAction.class),SystemAction.get(CopyAction.class),SystemAction.get(CutAction.class),SystemAction.get(PasteAction.class),SystemAction.get(DeleteAction.class)};
  }
}
