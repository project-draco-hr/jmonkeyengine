{
  if (!(info.getKey() instanceof TextureKey))   throw new IllegalArgumentException("Texture assets must be loaded using a TextureKey");
  boolean flip=((TextureKey)info.getKey()).isFlipY();
  InputStream in=null;
  try {
    in=info.openStream();
    Image img=load(in,flip);
    return img;
  }
  finally {
    if (in != null) {
      in.close();
    }
  }
}
