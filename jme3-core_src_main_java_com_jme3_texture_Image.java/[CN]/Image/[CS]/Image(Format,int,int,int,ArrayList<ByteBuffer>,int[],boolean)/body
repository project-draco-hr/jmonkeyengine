{
  this();
  if (mipMapSizes != null && mipMapSizes.length <= 1) {
    mipMapSizes=null;
  }
 else {
    needGeneratedMips=false;
    mipsWereGenerated=true;
  }
  setFormat(format);
  this.width=width;
  this.height=height;
  this.data=data;
  this.depth=depth;
  this.mipMapSizes=mipMapSizes;
  this.srgb=isSrgb;
}
