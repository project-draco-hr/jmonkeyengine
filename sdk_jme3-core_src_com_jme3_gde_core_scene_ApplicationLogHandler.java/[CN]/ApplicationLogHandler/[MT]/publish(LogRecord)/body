{
  if (record.getLevel().equals(Level.SEVERE)) {
    io.getErr().println(formatter.formatMessage(record));
  }
 else   if (record.getLevel().equals(Level.WARNING)) {
    io.getErr().println(formatter.formatMessage(record));
  }
 else {
    io.getOut().println(formatter.formatMessage(record));
  }
}
