{
  try {
    AnimControl clone=(AnimControl)super.clone();
    clone.spatial=spatial;
    if (skeleton != null) {
      clone.skeleton=new Skeleton(skeleton);
    }
    clone.channels=new ArrayList<AnimChannel>();
    return clone;
  }
 catch (  CloneNotSupportedException ex) {
    throw new AssertionError();
  }
}
