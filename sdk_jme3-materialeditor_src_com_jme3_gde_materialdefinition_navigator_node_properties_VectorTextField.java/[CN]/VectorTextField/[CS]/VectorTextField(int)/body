{
  setFocusTraversalKeysEnabled(false);
  addKeyListener(this);
  addFocusListener(this);
  addMouseListener(this);
  setFocusTraversalKeysEnabled(false);
  this.capacity=capacity;
  backup="[";
  for (int i=0; i < capacity; i++) {
    backup+="0";
    if (i != capacity - 1) {
      backup+=",";
    }
  }
  backup+="]";
}
