{
  BufferedImage retval=new BufferedImage(size,size,BufferedImage.TYPE_INT_ARGB);
  buff.rewind();
  for (int y=0; y < size; y++) {
    for (int x=0; x < size; x++) {
      int c=(int)(ShaderUtils.clamp(buff.get(),0,1) * 255);
      retval.setRGB(x,y,0xFF000000 | c << 16 | c << 8 | c);
    }
  }
  return retval;
}
