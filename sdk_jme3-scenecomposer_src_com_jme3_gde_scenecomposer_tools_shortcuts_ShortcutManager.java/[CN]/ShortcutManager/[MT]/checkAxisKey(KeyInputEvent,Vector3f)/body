{
  if (kie.getKeyCode() == KeyInput.KEY_X) {
    axisStore=Vector3f.UNIT_X;
    return true;
  }
 else   if (kie.getKeyCode() == KeyInput.KEY_Y) {
    axisStore=Vector3f.UNIT_Y;
    return true;
  }
 else   if (kie.getKeyCode() == KeyInput.KEY_Z) {
    axisStore=Vector3f.UNIT_Z;
    return true;
  }
  return false;
}
