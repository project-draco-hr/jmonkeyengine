{
  ProjectAssetManager pm=obj.getLookup().lookup(ProjectAssetManager.class);
  if (pm == null || loadingThread != Thread.currentThread()) {
    return;
  }
  FileObject obj=pm.getAssetFileObject(ak);
  if (obj != null && !assetList.contains(obj)) {
    assetList.add(obj);
    assetKeyList.add(ak);
  }
}
