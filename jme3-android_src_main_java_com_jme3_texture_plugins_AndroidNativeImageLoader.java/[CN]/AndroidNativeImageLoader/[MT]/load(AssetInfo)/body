{
  boolean flip=((TextureKey)info.getKey()).isFlipY();
  InputStream in=null;
  try {
    in=info.openStream();
    Image img=load(in,flip);
    if (img == null) {
      throw new AssetLoadException("The given image cannot be loaded " + info.getKey());
    }
    return img;
  }
  finally {
    if (in != null) {
      in.close();
    }
  }
}
