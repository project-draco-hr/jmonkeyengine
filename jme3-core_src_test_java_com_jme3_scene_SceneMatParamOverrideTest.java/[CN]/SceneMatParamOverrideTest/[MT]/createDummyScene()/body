{
  Node scene=new Node("Scene Node");
  Node a=new Node("A");
  Node b=new Node("B");
  Node c=new Node("C");
  Node d=new Node("D");
  Node e=new Node("E");
  Node f=new Node("F");
  Node g=new Node("G");
  Node h=new Node("H");
  Node j=new Node("J");
  scene.attachChild(a);
  scene.attachChild(b);
  a.attachChild(c);
  a.attachChild(d);
  b.attachChild(e);
  b.attachChild(f);
  c.attachChild(g);
  c.attachChild(h);
  c.attachChild(j);
  return scene;
}
