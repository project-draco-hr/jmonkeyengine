{
  if (initialized)   return;
  initialized=true;
  logger.log(Level.INFO,"Running on {0}",getFullName());
  if (!lowPermissions) {
    try {
      Natives.extractNativeLibs(getPlatform(),settings);
    }
 catch (    IOException ex) {
      logger.log(Level.SEVERE,"Error while copying native libraries",ex);
    }
  }
}
