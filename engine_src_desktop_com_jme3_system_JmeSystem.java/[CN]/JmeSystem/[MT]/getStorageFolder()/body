{
  if (lowPermissions) {
    throw new UnsupportedOperationException("File system access restricted");
  }
  if (storageFolder == null) {
    storageFolder=new File(System.getProperty("user.home"),".jme3");
    if (!storageFolder.exists()) {
      storageFolder.mkdir();
    }
  }
  return storageFolder;
}
