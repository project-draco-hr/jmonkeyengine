{
  if (!paramName.startsWith("m_")) {
    paramName="m_" + paramName;
  }
  Uniform u=shader.getUniform(paramName);
switch (type) {
case Texture2D:
case Texture3D:
case TextureArray:
case TextureCubeMap:
case Int:
    u.setValue(VarType.Int,value);
  break;
default :
u.setValue(type,value);
break;
}
}
