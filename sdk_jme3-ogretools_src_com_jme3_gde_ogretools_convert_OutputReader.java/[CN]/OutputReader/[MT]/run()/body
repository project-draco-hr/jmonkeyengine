{
  try {
    String line;
    while ((line=in.readLine()) != null) {
      if (line.trim().length() > 0) {
        if (progress != null) {
          progress.progress(line);
        }
 else {
          Logger.getLogger(OutputReader.class.getName()).log(Level.INFO,"{0}",line);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
