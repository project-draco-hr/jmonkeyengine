{
  try {
    String[] split=sta.getLine().split(":");
    if (split.length > 2) {
      path=split[2].trim();
    }
    name=split[1].trim();
  }
 catch (  ArrayIndexOutOfBoundsException e) {
    Logger.getLogger(DefinitionBlock.class.getName()).log(Level.WARNING,"Parsing error line " + sta.getLineNumber() + " : "+ sta.getLine());
  }
}
