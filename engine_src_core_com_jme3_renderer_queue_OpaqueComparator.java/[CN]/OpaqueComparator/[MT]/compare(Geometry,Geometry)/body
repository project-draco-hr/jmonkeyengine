{
  Material m1=o1.getMaterial();
  Material m2=o2.getMaterial();
  int compareResult=m2.getSortId() - m1.getSortId();
  if (compareResult == 0) {
    float d1=distanceToCam(o1);
    float d2=distanceToCam(o2);
    if (d1 == d2)     return 0;
 else     if (d1 < d2)     return -1;
 else     return 1;
  }
 else {
    return compareResult;
  }
}
