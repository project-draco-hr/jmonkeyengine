{
  Sheet sheet=super.createSheet();
  Sheet.Set set=Sheet.createPropertiesSet();
  set.setDisplayName(filter.getName());
  set.setName(Node.class.getName());
  Filter obj=filter;
  if (obj == null) {
    return sheet;
  }
  createFields(filter.getClass(),set,obj);
  sheet.put(set);
  return sheet;
}
