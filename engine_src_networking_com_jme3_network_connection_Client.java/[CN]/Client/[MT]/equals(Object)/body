{
  if (obj == null || !(obj instanceof Client || obj instanceof Integer)) {
    return false;
  }
 else   if (obj instanceof Client) {
    return ((Client)obj).getClientID() == getClientID();
  }
 else   if (obj instanceof Integer) {
    return ((Integer)obj).intValue() == getClientID();
  }
 else {
    return false;
  }
}
