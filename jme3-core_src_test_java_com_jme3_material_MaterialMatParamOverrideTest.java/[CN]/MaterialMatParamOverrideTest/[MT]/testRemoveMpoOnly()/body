{
  material("Common/MatDefs/Light/Lighting.j3md");
  reset();
  inputMpo(mpoInt("NumberOfBones",4321));
  outDefines(def("NUM_BONES",VarType.Int,4321));
  outUniforms(uniform("NumberOfBones",VarType.Int,4321));
  reset();
