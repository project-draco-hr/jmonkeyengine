{
  if (matrix.length != 3 || matrix[0].length != 3) {
    throw new IllegalArgumentException("Array must be of size 9.");
  }
  m00=matrix[0][0];
  m01=matrix[0][1];
  m02=matrix[0][2];
  m10=matrix[1][0];
  m11=matrix[1][1];
  m12=matrix[1][2];
  m20=matrix[2][0];
  m21=matrix[2][1];
  m22=matrix[2][2];
  return this;
}
