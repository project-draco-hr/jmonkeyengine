{
  Point p=SwingUtilities.convertPoint(this,e.getX(),e.getY(),panel);
  if (panel.contains(p)) {
    MouseEvent me=SwingUtilities.convertMouseEvent(this,e,panel);
    panel.mouseEntered(me);
  }
 else {
    MouseEvent me=SwingUtilities.convertMouseEvent(this,e,panel);
    panel.mouseExited(me);
  }
}
