{
switch (mode) {
case BorderClamp:
    return gl.GL_CLAMP_TO_BORDER;
case Clamp:
  return gl.GL_CLAMP;
case EdgeClamp:
return gl.GL_CLAMP_TO_EDGE;
case Repeat:
return gl.GL_REPEAT;
case MirroredRepeat:
return gl.GL_MIRRORED_REPEAT;
default :
throw new UnsupportedOperationException("Unknown wrap mode: " + mode);
}
}
