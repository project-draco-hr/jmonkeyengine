{
switch (buffer) {
case GL.GL_NONE:
    return "NONE";
case GL.GL_FRONT:
  return "GL_FRONT";
case GL.GL_BACK:
return "GL_BACK";
default :
if (buffer >= GL.GL_COLOR_ATTACHMENT0 && buffer <= GL2.GL_COLOR_ATTACHMENT15) {
return "GL_COLOR_ATTACHMENT" + (buffer - GL.GL_COLOR_ATTACHMENT0);
}
 else {
return "UNKNOWN? " + buffer;
}
}
}
