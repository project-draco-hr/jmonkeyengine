{
  if (data == null) {
    return null;
  }
  ByteBuffer buff=BufferUtils.createByteBuffer(4 * data.size());
  for (  byte[] v : data) {
    if (v != null) {
      buff.put(v[0]).put(v[1]).put(v[2]).put(v[3]);
    }
 else {
      buff.put((byte)0).put((byte)0).put((byte)0).put((byte)0);
    }
  }
  buff.flip();
  return buff;
}
