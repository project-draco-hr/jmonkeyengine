{
  allowSwapBuffers=settings.isSwapBuffers();
  if (renderable.get()) {
    if (!runningFirstTime) {
      if (!USE_SHARED_CONTEXT) {
        renderer.cleanup();
      }
    }
    super.createContext(settings);
  }
  if (runningFirstTime) {
    initContextFirstTime();
    runningFirstTime=false;
  }
}
