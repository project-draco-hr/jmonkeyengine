{
  int removedAssets=0;
  for (AssetRef ref; (ref=(AssetRef)refQueue.poll()) != null; ) {
    if (assetCache.remove(ref.assetKey) != null) {
      removedAssets++;
    }
  }
  if (removedAssets >= 1) {
    logger.log(Level.INFO,"WeakRefAssetCache: {0} assets were purged from the cache.",removedAssets);
  }
}
