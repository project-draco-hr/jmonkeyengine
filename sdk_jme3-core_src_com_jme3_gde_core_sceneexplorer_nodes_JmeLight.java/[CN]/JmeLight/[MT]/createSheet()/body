{
  Sheet sheet=Sheet.createDefault();
  Sheet.Set set=Sheet.createPropertiesSet();
  set.setDisplayName("Light");
  set.setName(Light.class.getName());
  Light obj=light;
  if (obj == null) {
    return sheet;
  }
  set.put(makeProperty(obj,ColorRGBA.class,"getColor","setColor","Color"));
  sheet.put(set);
  return sheet;
}
