{
  if (!scan.hasNext("\\{")) {
    texName=readString("\\{");
  }
 else {
    texName=null;
  }
  scan.next();
  if (!skipIt) {
    texture=new Texture2D();
  }
  while (!scan.hasNext("\\}")) {
    if (skipIt) {
      readString("\n");
    }
 else {
      readTextureUnitStatement();
    }
  }
  scan.next();
}
