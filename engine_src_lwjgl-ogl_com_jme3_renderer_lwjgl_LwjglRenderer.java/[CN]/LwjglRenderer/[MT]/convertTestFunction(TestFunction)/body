{
switch (testFunc) {
case Never:
    return GL_NEVER;
case Less:
  return GL_LESS;
case LessOrEqual:
return GL_LEQUAL;
case Greater:
return GL_GREATER;
case GreaterOrEqual:
return GL_GEQUAL;
case Equal:
return GL_EQUAL;
case NotEqual:
return GL_NOTEQUAL;
case Always:
return GL_ALWAYS;
default :
throw new UnsupportedOperationException("Unrecognized test function: " + testFunc);
}
}
