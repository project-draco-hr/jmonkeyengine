{
  if (!(o instanceof DQuaternion)) {
    return false;
  }
  if (this == o) {
    return true;
  }
  DQuaternion comp=(DQuaternion)o;
  if (Double.compare(x,comp.x) != 0) {
    return false;
  }
  if (Double.compare(y,comp.y) != 0) {
    return false;
  }
  if (Double.compare(z,comp.z) != 0) {
    return false;
  }
  if (Double.compare(w,comp.w) != 0) {
    return false;
  }
  return true;
}
