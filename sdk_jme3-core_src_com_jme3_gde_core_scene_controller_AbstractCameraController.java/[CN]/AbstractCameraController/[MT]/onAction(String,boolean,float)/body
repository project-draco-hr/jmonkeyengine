{
  if ("MouseButtonLeft".equals(string)) {
    if (bln) {
      leftMouse=true;
      moved=false;
    }
 else {
      leftMouse=false;
      if (!moved) {
        checkClick=true;
      }
    }
  }
  if ("MouseButtonRight".equals(string)) {
    if (bln) {
      rightMouse=true;
      movedR=false;
    }
 else {
      rightMouse=false;
      if (!movedR) {
        checkClickR=true;
      }
    }
  }
}
