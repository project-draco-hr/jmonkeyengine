{
  InputStream stream=null;
  try {
    AssetConfig cfg=new AssetConfig(this);
    stream=configFile.openStream();
    cfg.loadText(stream);
  }
 catch (  IOException ex) {
    logger.log(Level.SEVERE,"Failed to load asset config",ex);
  }
 finally {
    if (stream != null)     try {
      stream.close();
    }
 catch (    IOException ex) {
    }
  }
}
