{
  ensureParent(targetFile);
  InputStream ins=null;
  OutputStream out=null;
  try {
    ins=new FileInputStream(sourceFile);
    out=new FileOutputStream(targetFile);
    FileUtil.copy(ins,out);
  }
  finally {
    if (ins != null) {
      ins.close();
    }
    if (out != null) {
      out.close();
    }
  }
}
