{
  byte rbyte;
  int read;
  for (int i=off; i < length; ) {
    read=tr(chars[i++]);
    if (read >= 0)     rbyte=(byte)(read << 4);
 else     continue;
    while (i < length) {
      read=tr(chars[i++]);
      if (read >= 0) {
        rbyte+=(byte)read;
        os.write(rbyte);
        break;
      }
    }
  }
}
