{
  elementStack.clear();
  indexToBone.clear();
  nameToBone.clear();
  track=null;
  tracks.clear();
  animation=null;
  if (animations != null)   animations.clear();
  bone=null;
  skeleton=null;
  times.clear();
  rotations.clear();
  translations.clear();
  time=-1;
  position=null;
  rotation=null;
  scale=null;
  angle=0;
  axis=null;
}
