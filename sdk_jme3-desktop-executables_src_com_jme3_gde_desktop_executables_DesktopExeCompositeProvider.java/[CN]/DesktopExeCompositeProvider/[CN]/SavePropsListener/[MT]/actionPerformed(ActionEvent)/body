{
  if ("true".equals(properties.getProperty("windows-x86.app.enabled")) || "true".equals(properties.getProperty("windows-x64.app.enabled")) || "true".equals(properties.getProperty("linux-x86.app.enabled"))|| "true".equals(properties.getProperty("linux-x64.app.enabled"))|| "true".equals(properties.getProperty("macosx-x64.app.enabled"))) {
    desktopDeployment.loadTargets("nbres:/com/jme3/gde/desktop/executables/desktop-deployment-targets.xml");
    desktopDeployment.checkExtension(project);
    if ("true".equals(properties.getProperty("bundle.jre.enabled"))) {
      checkJreDownloads();
    }
  }
 else {
    desktopDeployment.removeExtension(project);
  }
  try {
    properties.store();
  }
 catch (  IOException ioe) {
    Exceptions.printStackTrace(ioe);
  }
}
