{
  super.redo();
  try {
    SceneApplication.getApplication().enqueue(new Callable<Void>(){
      public Void call() throws Exception {
        sceneRedo();
        return null;
      }
    }
).get();
  }
 catch (  InterruptedException ex) {
    Exceptions.printStackTrace(ex);
  }
catch (  ExecutionException ex) {
    Exceptions.printStackTrace(ex);
  }
  awtRedo();
}
