{
  if (isRunning)   throw new IllegalStateException("Server is already started.");
  if (reliable != null) {
    reliable.initialize();
  }
  if (fast != null) {
    fast.initialize();
  }
  if (reliableAdapter != null) {
    reliableAdapter.start();
  }
  if (fastAdapter != null) {
    fastAdapter.start();
  }
  isRunning=true;
}
