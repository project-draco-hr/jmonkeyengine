{
  UIManager.installLookAndFeel(new UIManager.LookAndFeelInfo(new DarkMonkeyLookAndFeel().getName(),DarkMonkeyLookAndFeel.class.getName()));
  String[] fontsToLoad={"fonts/DejaVuSans.ttf","fonts/DejaVuSans-Bold.ttf","fonts/DejaVuSans-Oblique.ttf","fonts/DejaVuSans-BoldOblique.ttf","fonts/DejaVuSansCondensed.ttf","fonts/DejaVuSansCondensed-Bold.ttf","fonts/DejaVuSansCondensed-Oblique.ttf","fonts/DejaVuSansCondensed-BoldOblique.ttf","fonts/DejaVuSansMono.ttf","fonts/DejaVuSansMono-Bold.ttf","fonts/DejaVuSansMono-Oblique.ttf","fonts/DejaVuSansMono-BoldOblique.ttf"};
  DMUtils.loadFontsFromJar(this,fontsToLoad);
}
