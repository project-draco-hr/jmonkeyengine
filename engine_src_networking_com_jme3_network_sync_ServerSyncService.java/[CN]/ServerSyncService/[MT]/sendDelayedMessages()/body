{
  ArrayList<Long> removeList=new ArrayList<Long>();
  for (  Map.Entry<Long,SyncMessage> entry : latencyQueue.entrySet()) {
    if (entry.getKey() > System.currentTimeMillis())     continue;
    removeList.add(entry.getKey());
    if (packetDropRate > FastMath.nextRandomFloat())     continue;
    for (    Client client : server.getConnectors()) {
      try {
        client.send(entry.getValue());
      }
 catch (      IOException ex) {
        ex.printStackTrace();
      }
    }
  }
  for (  Long removeEntry : removeList)   latencyQueue.remove(removeEntry);
}
