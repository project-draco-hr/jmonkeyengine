{
  this.owner=info.getManager();
  InputStream in=info.openStream();
  try {
    scan=new Scanner(in);
    scan.useLocale(Locale.US);
    key=info.getKey();
    loadFromScanner();
  }
  finally {
    if (in != null)     in.close();
  }
  if (material != null) {
    if (!(info.getKey() instanceof MaterialKey)) {
      throw new IOException("Material instances must be loaded via MaterialKey");
    }
    return material;
  }
 else {
    return materialDef;
  }
}
