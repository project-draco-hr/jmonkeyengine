{
  String name=null;
  if (!scan.hasNext("\\{")) {
    name=scan.next();
  }
  technique=new TechniqueDef(name);
  String word=scan.next();
  throwIfNequal("{",word);
  nextStatement();
  while (true) {
    if (scan.hasNext("\\}")) {
      scan.next();
      break;
    }
    readTechniqueStatement();
  }
  if (vertName != null && fragName != null) {
    technique.setShaderFile(vertName,fragName,shaderLang);
  }
  materialDef.addTechniqueDef(technique);
  technique=null;
  vertName=null;
  fragName=null;
  shaderLang=null;
}
