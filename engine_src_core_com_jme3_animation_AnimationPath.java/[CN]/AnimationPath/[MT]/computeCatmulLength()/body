{
  float l=0;
  if (wayPoints.size() > 1) {
    for (int i=0; i < wayPoints.size() - 1; i++) {
      l=getCatmullRomP1toP2Length(CRcontrolPoints.get(i),CRcontrolPoints.get(i + 1),CRcontrolPoints.get(i + 2),CRcontrolPoints.get(i + 3),0,1);
      segmentsLength.add(l);
      totalLength+=l;
    }
  }
  speed=totalLength / duration;
}
