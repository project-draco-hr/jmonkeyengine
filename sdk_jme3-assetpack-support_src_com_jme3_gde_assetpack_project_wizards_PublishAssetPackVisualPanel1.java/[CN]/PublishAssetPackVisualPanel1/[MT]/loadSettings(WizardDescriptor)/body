{
  AssetPackProject project=(AssetPackProject)wiz.getProperty("project");
  jTextArea1.setText(project.getDescription());
  jLabel1.setText(project.getProjectName());
  jLabel3.setText(project.getDistributorName());
  jLabel4.setText(project.getVersion());
  jTextField1.setText((String)wiz.getProperty("filename"));
}
