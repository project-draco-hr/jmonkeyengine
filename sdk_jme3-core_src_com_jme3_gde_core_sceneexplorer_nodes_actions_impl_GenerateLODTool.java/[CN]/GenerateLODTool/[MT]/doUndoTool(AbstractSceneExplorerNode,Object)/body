{
  Geometry geom=rootNode.getLookup().lookup(Geometry.class);
  Mesh mesh=geom.getMesh();
  if (mesh != null) {
    VertexBuffer[] vbs=(VertexBuffer[])undoObject;
    if (vbs.length > 0) {
      mesh.setLodLevels((VertexBuffer[])undoObject);
    }
 else {
      mesh.setLodLevels(null);
    }
  }
}
