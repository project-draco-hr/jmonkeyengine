{
switch (bucket) {
case Gui:
    return guiList.size() == 0;
case Opaque:
  return opaqueList.size() == 0;
case Sky:
return skyList.size() == 0;
case Transparent:
return transparentList.size() == 0;
case Translucent:
return translucentList.size() == 0;
default :
throw new UnsupportedOperationException("Unsupported bucket type: " + bucket);
}
}
