{
switch (bucket) {
case Gui:
    guiList.add(g);
  break;
case Opaque:
opaqueList.add(g);
break;
case Sky:
skyList.add(g);
break;
case Transparent:
transparentList.add(g);
break;
case Translucent:
translucentList.add(g);
break;
default :
throw new UnsupportedOperationException("Unknown bucket type: " + bucket);
}
}
