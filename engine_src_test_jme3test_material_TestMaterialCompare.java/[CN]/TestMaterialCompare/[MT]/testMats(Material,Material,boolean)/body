{
  if (mat2.equals(mat1)) {
    System.out.print(mat1.getName() + " equals " + mat2.getName());
    if (expected) {
      System.out.println(" success");
    }
 else {
      System.out.println(" fail");
    }
  }
 else {
    System.out.print(mat1.getName() + " is not equal " + mat2.getName());
    if (!expected) {
      System.out.println(" success");
    }
 else {
      System.out.println(" fail");
    }
  }
}
