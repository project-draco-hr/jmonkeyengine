{
  if (attributes == null)   return;
  int i, k=attributes.names.size();
  for (i=0; i < k; i++) {
    if (!attributes.oneLine)     sb.append(indentation);
    sb.append(attributes.names.get(i)).append("=\"").append(attributes.values.get(i)).append('\"');
    if (!attributes.oneLine)     sb.append("\n");
 else     if (i < (k - 1))     sb.append(' ');
  }
}
