{
  Field[] fields=clazz.getDeclaredFields();
  for (  Field field : fields) {
    if ((field.getModifiers() & (Modifier.STATIC | Modifier.PUBLIC)) != 0) {
      String name=field.getName();
      Class<?> type=field.getType();
      if (type == boolean.class) {
        capsSet.add(name);
      }
    }
  }
}
