{
  int counter=1;
  while (sequence.movePrevious() && counter > 0) {
    if (sequence.token().id() == id) {
      if (TokenUtilities.equals(sequence.token().text(),stopToken)) {
        counter--;
      }
 else       if (TokenUtilities.equals(sequence.token().text(),countToken)) {
        counter++;
      }
    }
  }
}
