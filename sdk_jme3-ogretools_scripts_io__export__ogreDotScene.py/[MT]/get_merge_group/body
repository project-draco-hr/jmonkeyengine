def get_merge_group(ob, prefix='merge'):
    m = []
    for grp in ob.users_group:
        if grp.name.lower().startswith(prefix):
            m.append(grp)
    if (len(m) == 1):
        return m[0]
    elif m:
        print ('WARNING: an object can not be in two merge groups at the same time', ob)
        return
