def load(self, context, url, clear=False):
    self._objects += [ob.name for ob in context.selected_objects]
    if clear:
        self.proc.stdin.write('loadscene(/tmp/rex/preview.txml,true,true)\n')
    else:
        self.proc.stdin.write('loadscene(/tmp/rex/preview.txml,false,true)\n')
    try:
        self.proc.stdin.flush()
    except:
        global TundraSingleton
        TundraSingleton = None
