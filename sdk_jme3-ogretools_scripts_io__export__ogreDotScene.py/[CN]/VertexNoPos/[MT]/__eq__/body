def __eq__(self, o):
    if ((self.nx != o.nx) or (self.ny != o.ny) or (self.nz != o.nz)):
        return False
    elif ((self.r != o.r) or (self.g != o.g) or (self.b != o.b) or (self.ra != o.ra)):
        return False
    elif (len(self.vert_uvs) != len(o.vert_uvs)):
        return False
    elif self.vert_uvs:
        for (i, uv1) in enumerate(self.vert_uvs):
            uv2 = o.vert_uvs[i]
            if (uv1 != uv2):
                return False
    return True
