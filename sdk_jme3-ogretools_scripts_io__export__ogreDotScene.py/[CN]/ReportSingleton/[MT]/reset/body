def reset(self):
    self.materials = []
    self.meshes = []
    self.lights = []
    self.cameras = []
    self.armatures = []
    self.armature_animations = []
    self.shape_animations = []
    self.textures = []
    self.vertices = 0
    self.orig_vertices = 0
    self.faces = 0
    self.triangles = 0
    self.warnings = []
    self.errors = []
    self.messages = []
    self.paths = []
