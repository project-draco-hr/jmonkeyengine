{
  super.onResume();
  if (view != null) {
    view.onResume();
  }
  if (app != null) {
    AudioRenderer result=app.getAudioRenderer();
    if (result != null) {
      if (result instanceof AndroidAudioRenderer) {
        AndroidAudioRenderer renderer=(AndroidAudioRenderer)result;
        renderer.resumeAll();
      }
    }
  }
  isGLThreadPaused=false;
  logger.info("onResume");
}
