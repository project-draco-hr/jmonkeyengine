{
  StringBuilder sb=new StringBuilder();
  sb.append("<code>");
  if (content.type != null) {
    sb.append(content.type);
    sb.append(" ");
  }
  sb.append("<b>");
  sb.append(item);
  sb.append("</b>");
  if (content.arguments != null) {
    sb.append(content.arguments);
  }
  sb.append("</code><p>");
  sb.append(content.doc);
  sb.append("</p>");
  text=sb.toString();
}
