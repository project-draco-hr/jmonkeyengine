{
  int type=Info.clGetDeviceInfoInt(device.address(),CL10.CL_DEVICE_TYPE);
switch (type) {
case CL10.CL_DEVICE_TYPE_ACCELERATOR:
    return DeviceType.ACCELEARTOR;
case CL10.CL_DEVICE_TYPE_CPU:
  return DeviceType.CPU;
case CL10.CL_DEVICE_TYPE_GPU:
return DeviceType.GPU;
default :
return DeviceType.DEFAULT;
}
}
