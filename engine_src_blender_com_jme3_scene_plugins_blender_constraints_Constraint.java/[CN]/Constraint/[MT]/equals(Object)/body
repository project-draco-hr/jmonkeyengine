{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Constraint other=(Constraint)obj;
  if (name == null) {
    if (other.name != null) {
      return false;
    }
  }
 else   if (!name.equals(other.name)) {
    return false;
  }
  if (ownerOMA == null) {
    if (other.ownerOMA != null) {
      return false;
    }
  }
 else   if (!ownerOMA.equals(other.ownerOMA)) {
    return false;
  }
  return true;
}
