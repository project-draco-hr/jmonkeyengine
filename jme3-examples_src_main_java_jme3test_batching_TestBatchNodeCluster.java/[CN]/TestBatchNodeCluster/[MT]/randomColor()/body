{
  ColorRGBA color=ColorRGBA.Black;
  int randomn=rand.nextInt(4);
  if (randomn == 0) {
    color=ColorRGBA.Orange;
  }
 else   if (randomn == 1) {
    color=ColorRGBA.Blue;
  }
 else   if (randomn == 2) {
    color=ColorRGBA.Brown;
  }
 else   if (randomn == 3) {
    color=ColorRGBA.Magenta;
  }
  return color;
}
