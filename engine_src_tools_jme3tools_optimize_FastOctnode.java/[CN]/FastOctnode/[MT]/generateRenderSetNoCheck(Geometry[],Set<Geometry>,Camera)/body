{
  if (length != 0) {
    int start=getOffset();
    int end=start + length;
    for (int i=start; i < end; i++) {
      renderSet.add(globalGeomList[i]);
    }
  }
  if (child == null)   return;
  FastOctnode node=child;
  while (node != null) {
    node.generateRenderSetNoCheck(globalGeomList,renderSet,cam);
    node=node.next;
  }
}
